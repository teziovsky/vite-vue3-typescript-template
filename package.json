{
  "name": "vite-vue3-typescript-template",
  "version": "1.0.0",
  "description": "A Vite with Vue3 and TypeScript starter template. Included eslint, prettier, husky and lint-staged.",
  "homepage": "https://github.com/teziovsky/vite-vue3-typescript-template/#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:teziovsky/vite-vue3-typescript-template.git"
  },
  "engines": {
    "node": ">=v14.0.0",
    "npm": ">=v6.0.0"
  },
  "bugs": {
    "url": "https://github.com/teziovsky/vite-vue3-typescript-template/issues",
    "email": "teziovsky@gmail.com"
  },
  "license": "MIT License",
  "author": {
    "name": "Jakub Soboczy≈Ñski",
    "email": "jakub.soboczynski@icloud.com",
    "url": "https://www.jakubsoboczynski.pl"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "node_modules/eslint/bin/eslint.js --fix .",
    "format": "node_modules/prettier/bin-prettier.js --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx,json,md}": "prettier --cache --write",
    "*.{vue,js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "typescript": "^4.3.2",
    "vite": "^2.3.4",
    "vue-tsc": "^0.0.24"
  }
}